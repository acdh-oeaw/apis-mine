    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django runserver",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        },
        {
            "name": "rebuild index",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "rebuild_index",
                "--noinput"
            ],
            "django": true
        },
        {
				"name": "Paas remote Jupyter",
				"type": "python",
				"request": "launch",
				"program": "${workspaceFolder}/manage.py",
				"args": [
					"shell_plus",
					"--notebook",
					"--settings=apis.settings.paas_remote"
				],
				"django": true
			},
    ]
}